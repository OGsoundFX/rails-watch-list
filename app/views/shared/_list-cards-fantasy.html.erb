<ul class="cards">
  <% @lists.each do |list| %>
    <li>
      <%= link_to(list_path(list)) do %>
        <div class="card">
          <% if list.movies == [] %>
            <img src="https://res.cloudinary.com/dk9a86uhu/image/upload/v1636642178/krvbal8e1sawpf11noa11xnvazc0.jpg" class="card__image" alt="" />
          <% else %>
            <img src="<%= list.movies.first.poster_url %>" class="card__image" alt="" />
          <% end %>
          <div class="card__overlay">
            <div class="card__header">
              <svg class="card__arc" xmlns="http://www.w3.org/2000/svg"><path /></svg>                     
              <img class="card__thumb" src="data:image/jpeg;base64," alt="" />
              <div class="card__header-text">
                <h3 class="card__title"><%= list.name %></h3>            
                <span class="card__status"><%= pluralize(list.movies.count, 'movie') %></span>
              </div>
            </div>
            <p class="card__description"><%= list.description %></p>
          </div>
        </div>
      <% end %>
    </li>
  <% end %>
</ul>
